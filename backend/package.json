{
  "name": "peeppipsbackend",
  "version": "1.0.0",
  "description": "A simple Temporal workflow example using Node.js",
  "type": "module",
  "scripts": {
    "start": "concurrently -n SERVER,WORKER -c yellow,cyan \"npm run start:server\" \"npm run start:worker\"",
    "start:server": "npx tsx src/server/index.ts",
    "start:worker": "npx tsx src/temporal/workers/worker.ts",
    "dev": "concurrently -n SERVER,WORKER -c yellow,cyan \"npm run dev:server\" \"npm run dev:worker\"",
    "dev:server": "nodemon --exec \"npm run start:server\"",
    "dev:worker": "nodemon --exec npm run temporal-worker",
    "temporal-worker": "npx tsx src/temporal/workers/worker.ts",
    "workflow": "npx tsx src/temporal/clients/client.ts",
    "docker-dev": "docker compose -f compose.local.yaml up --build",
    "docker-prod": "docker compose -f compose.yaml up --build",
    "drizzle-generate": "npx drizzle-kit generate --dialect=postgresql --schema=./src/server/db/schema.ts",
    "drizzle-push": "npx drizzle-kit push",
    "drizzle-pull": "npx drizzle-kit pull",
    "drizzle-migrate": "npx drizzle-kit migrate",
    "cache-gemini": "npx tsx src/server/utils/utils.generateGeminiCache.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
    "@getzep/zep-cloud": "^2.18.0",
    "@google-cloud/storage": "^7.16.0",
    "@google/genai": "^1.8.0",
    "@google/generative-ai": "^0.24.1",
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "@qdrant/js-client-rest": "^1.15.0",
    "@supabase/supabase-js": "^2.50.2",
    "@temporalio/activity": "^1.11.7",
    "@temporalio/client": "^1.11.7",
    "@temporalio/worker": "^1.11.7",
    "@temporalio/workflow": "^1.11.7",
    "@types/express": "^5.0.1",
    "@types/pg": "^8.11.14",
    "ai": "^4.3.16",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "openai": "^5.8.2",
    "pg": "^8.15.6",
    "postgres": "^3.4.5",
    "prisma": "^6.7.0",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "tsx": "^4.19.4",
    "uuid": "^11.1.0",
    "zod": "^3.25.61"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.9",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.3"
  }
}
